#include <stdio.h>

int main() {
    int n, i;
    char name[50];   // to store the name

    printf("Enter your name: ");
    scanf("%s", name);   // reads one word name (like "Yashvee")

    printf("Enter how many times you want to print your name: ");
    scanf("%d", &n);

    printf("\nPrinting your name %d times:\n", n);

    for(i = 1; i <= n; i++) {
        printf("%d. %s\n", i, name);
    }

    return 0;
}
