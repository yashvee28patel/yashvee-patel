#include <stdio.h>

int main() {
    int n, i;

    printf("Enter how many even numbers you want to print: ");
    scanf("%d", &n);

    printf("The first %d even numbers are:\n", n);

    for(i = 1; i <= n; i++) {
        printf("%d ", 2 * i);   // formula for even numbers
    }

    printf("\n");
    return 0;
}
